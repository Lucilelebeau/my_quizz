{% extends 'base.html.twig' %}

{% block title %}Reponse{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Question </h1>

    <ul>
        {% for reponse in reponses %}
            <li><code><a href="">{{ loop.index }} - {{ reponse.id_question }} {{ reponse.id }} {{ reponse.reponse }} {{ reponse.reponse_expected }}</a><button type="submit">OK</button></code></li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
